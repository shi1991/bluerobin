{
  "extends": "google",
  "plugins": ["html"],
  "rules": {
    "no-undef": 0,
    "linebreak-style": 0,
    "indent": [2, 2],
    "camelcase": 0,
    "max-len": 0,
    "no-unused-vars": 1,
    "arrow-parens": [2, "always"],
    "comma-dangle": [0, "always"],
    "require-jsdoc": 0,
    "object-curly-spacing": [0, "always"]
  },
  "globals": {
    "window": true,
    "localStorage": true
  }
}
